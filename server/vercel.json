{
    "version": 2,
    "name": "music-album",
    "builds": [
       { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
       { 
         "src": "/(.*)", 
         "dest": "/app.js",
         "methods":["GET","POST","PUT","PATCH","DELETE"],
         "headers": {
            "Access-Control-Allow-Origin": "*"
         }
      }
    ]
 }