{
  "version": 2,
  "name": "TravelSaga",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build"
      
    },
    {
      "src": "server/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/build/$1"
    }
  ]
}
